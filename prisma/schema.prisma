generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String @id @default(cuid())
  userName     String
  userPassword String
  toDos        toDo[]

  @@unique([id, userName])
}

model toDo {
  id     String @id @default(cuid())
  text   String
  userId String
  completed Boolean
  
  user   User   @relation(fields: [userId], references: [id])
}
