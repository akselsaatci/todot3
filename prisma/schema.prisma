generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           String @id @default(cuid())
  userName     String
  userPassword String
  @@unique([id, userName])
}

model toDo {
  id     String @id @default(cuid())
  text   String
  userId String
  completed Boolean  
}
